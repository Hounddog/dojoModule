{
    "name": "superdweebie/DojoModule",
    "description": "Provides Dojo 1.7 module for zf2",
    "type": "library",
    "keywords": [
        "dojo",
        "zf2"
    ],
    "homepage": "http://github.com/superdweebie/DojoModule",
    "authors": [
        {
            "name": "Tim Roediger",
            "email": "superdweebie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/console": "2.0.*",
        "dojo/dojo" : "1.7.*",
        "dojo/dijit" : "1.7.*",
        "dojo/dojox" : "1.7.*",
        "dojo/util" : "1.7.*"         
    },

    "autoload": {
        "psr-0": {
            "DojoModule": "src/"
        },
        "classmap": [
            "./"
        ]
    },
    "bin": [
        "bin/dojo-module"
    ]    
}